id === id + color) {\n          let newAmount = cartItem.amount + amount;\n          if (newAmount > cartItem.max) {\n            newAmount = cartItem.max;\n          }\n          return { ...cartItem, amount: newAmount };\n        } else {\n          return cartItem;\n        }\n      });\n\n      return { ...state, cart: tempCart };\n    } else {\n      const newItem = {\n        id: id + color,\n        name: product.name,\n        color,\n        amount,\n        image: product.images[0].url,\n        price: product.price,\n        max: product.stock,\n      };\n      return { ...state, cart: [...state.cart, newItem] };\n    }\n  }\n  if (action.type === REMOVE_CART_ITEM) {\n    const tempCart = state.cart.filter((item) => item.id !== action.payload);\n    return { ...state, cart: tempCart };\n  }\n  if (action.type === CLEAR_CART) {\n    return { ...state, cart: [] };\n  }\n  if (action.type === TOGGLE_CART_ITEM_AMOUNT) {\n    const { id, value } = action.payload;\n    const tempCart = state.cart.map((item) => {\n      if (item.id === id) {\n        if (value === \"inc\") {\n          let newAmount = item.amount + 1;\n          if (newAmount > item.max) {\n            newAmount = item.max;\n          }\n          return { ...item, amount: newAmount };\n        }\n        if (value === \"dec\") {\n          let newAmount = item.amount - 1;\n          if (newAmount < 1) {\n            newAmount = 1;\n          }\n          return { ...item, amount: newAmount };\n        }\n      } else {\n        return item;\n      }\n    });\n\n    return { ...state, cart: tempCart };\n  }\n  if (action.type == COUNT_CART_TOTALS) {\n    const { total_items, total_amount } = state.cart.reduce(\n      (total, cartItem) => {\n        const { amount, price } = cartItem;\n\n        total.total_items += amount;\n        total.total_amount += price * amount;\n        return total;\n      },\n      {\n        total_items: 0,\n        total_amount: 0,\n      }\n    );\n    return { ...state, total_items, total_amount };\n  }\n  throw new Error(`No Matching \"${action.type}\" - action type`);\n};\n\nexport default cart_reducer;\n","/home/mandi/mandi/e-commerce/src/reducers/filter_reducer.js",[],"/home/mandi/mandi/e-commerce/src/utils/constants.js",[],"/home/mandi/mandi/e-commerce/src/components/index.js",[],"/home/mandi/mandi/e-commerce/src/pages/index.js",[],"/home/mandi/mandi/e-commerce/src/pages/PrivateRoute.js",[],"/home/mandi/mandi/e-commerce/src/components/Navbar.js",[],"/home/mandi/mandi/e-commerce/src/components/FeaturedProducts.js",["206"],"/home/mandi/mandi/e-commerce/src/components/Sidebar.js",[],"/home/mandi/mandi/e-commerce/src/components/Footer.js",[],"/home/mandi/mandi/e-commerce/src/components/Hero.js",[],"/home/mandi/mandi/e-commerce/src/components/Services.js",[],"/home/mandi/mandi/e-commerce/src/components/Contact.js",[],"/home/mandi/mandi/e-commerce/src/components/Loading.js",[],"/home/mandi/mandi/e-commerce/src/components/Error.js",[],"/home/mandi/mandi/e-commerce/src/components/ProductImages.js",["207"],"/home/mandi/mandi/e-commerce/src/components/AddToCart.js",[],"/home/mandi/mandi/e-commerce/src/components/ProductList.js",[],"/home/mandi/mandi/e-commerce/src/components/Filters.js",[],"/home/mandi/mandi/e-commerce/src/components/Sort.js",[],"/home/mandi/mandi/e-commerce/src/components/Stars.js",[],"/home/mandi/mandi/e-commerce/src/components/PageHero.js",[],"/home/mandi/mandi/e-commerce/src/pages/HomePage.js",[],"/home/mandi/mandi/e-commerce/src/pages/SingleProductPage.js",["208","209"],"/home/mandi/mandi/e-commerce/src/pages/ProductsPage.js",[],"/home/mandi/mandi/e-commerce/src/pages/AboutPage.js",[],"/home/mandi/mandi/e-commerce/src/pages/CartPage.js",[],"/home/mandi/mandi/e-commerce/src/pages/ErrorPage.js",[],"/home/mandi/mandi/e-commerce/src/pages/CheckoutPage.js",[],"/home/mandi/mandi/e-commerce/src/components/Product.js",[],"/home/mandi/mandi/e-commerce/src/components/AmountButtons.js",[],"/home/mandi/mandi/e-commerce/s